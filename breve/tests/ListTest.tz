@use Control.

Controller ListTest.

Control : ListTest {
	+ to init:
		newlist (list).

		newlist = (self make-list).

		if | newlist | != 2: die "Wrong number of elements in $newlist".

		self end-simulation.

	+ to make-list:
		local (list).

		push "1" onto local.
		push "3" onto local.

		insert "2" at local{1}.

		print local.

		remove local{2}.

		return local.
}

