@use Control.

Controller ListTest.

Control : ListTest {
	+ to init:
		newlist (list).
		newhash (hash).

		newlist = (self make-list).

		newhash{ "dog" } = 1.
		newhash{ 10 } = 2.
		newhash{ self } = 2.

		print keys( newhash ).

		if | newlist | != 2: die "Wrong number of elements in $newlist".

		self end-simulation.

	+ to make-list:
		local (list).

		push "1" onto local.
		push "3" onto local.

		insert "2" at local{1}.

		print local.

		remove local{2}.

		return local.
}

