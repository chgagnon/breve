all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: breve.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) breve.pro
qmake: 
	@$(QMAKE) breve.pro

dist: 
	@mkdir -p .obj/breve && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/breve/ && ( cd `dirname .obj/breve` && $(TAR) breve.tar breve && $(GZIP) breve.tar ) && mv `dirname .obj/breve`/breve.tar.gz . && rm -rf .obj/breve

mocclean:
	-rm -f $(OBJMOC)
	-rm -f $(SRCMOC)

uiclean:
	-rm -f $(UICIMPLS) $(UICDECLS)

clean: mocclean uiclean
	-rm -f $(OBJECTS) 
		-rm -f .ui/qmake_image_collection.cpp
	-rm -f *~ core *.core


####### Sub-libraries

