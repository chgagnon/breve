	@mkdir -p .obj/breve && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/breve/ && $(COPY_FILE) --parents images/fileopen images/filesave images/undo images/redo images/editcut images/editcopy images/editpaste images/searchfind app_icon.png pause.png play.png stop.png .obj/breve/ && $(COPY_FILE) --parents brqtMainWindow.ui.h brqtEditorWindow.ui.h brqtErrorWindow.ui.h .obj/breve/ && ( cd `dirname .obj/breve` && $(TAR) breve.tar breve && $(GZIP) breve.tar ) && $(MOVE) `dirname .obj/breve`/breve.tar.gz . && $(DEL_FILE) -r .obj/breve

