all: Makefile breve.app/Contents/MacOS/../PkgInfo breve.app/Contents/MacOS/../Info.plist $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	test -d breve.app/Contents/MacOS/ || mkdir -p breve.app/Contents/MacOS/
