Makefile: breve.pro  /Developer/qt/mkspecs/default/qmake.conf /Developer/qt/lib/libqt.prl
	$(QMAKE) -o Makefile breve.pro
qmake: 
	@$(QMAKE) -o Makefile breve.pro

breve.app/Contents/MacOS/../PkgInfo: 
	@test -d breve.app/Contents/MacOS/ || mkdir -p breve.app/Contents/MacOS/
	@$(DEL_FILE) breve.app/Contents/MacOS/../PkgInfo
	@echo "APPL????" >breve.app/Contents/MacOS/../PkgInfo
breve.app/Contents/MacOS/../Info.plist: 
	@test -d breve.app/Contents/MacOS/ || mkdir -p breve.app/Contents/MacOS/
	@$(DEL_FILE) breve.app/Contents/MacOS/../Info.plist
	@sed -e "s,@ICON@,application.icns,g" -e "s,@EXECUTABLE@,breve,g" "/Developer/qt/mkspecs/default/Info.plist.app" >"breve.app/Contents/MacOS/../Info.plist"
